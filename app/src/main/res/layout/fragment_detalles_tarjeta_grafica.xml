<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DetallesTarjetaGrafica">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/L_Descripcion"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Descripcion"
            android:textColor="#97000000"
            android:textSize="12dp"
            android:textStyle="bold"
            android:background="#D8D8D8"
            android:padding="5dp"/>
        <TextView
            android:id="@+id/Tb_Descripcion"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="15dp"
            android:padding="5dp"/>
        <TextView
            android:id="@+id/L_Marca"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Marca"
            android:textColor="#97000000"
            android:textSize="12dp"
            android:background="#D8D8D8"
            android:padding="5dp"
            android:textStyle="bold"/>
        <TextView
            android:id="@+id/Tb_Marca"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="15dp"
            android:padding="5dp"/>
        <TextView
            android:id="@+id/L_TipoSalida"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Tipo Salida"
            android:textColor="#97000000"
            android:textSize="12dp"
            android:textStyle="bold"
            android:background="#D8D8D8"
            android:padding="5dp"/>
        <TextView
            android:id="@+id/Tb_TipoSalida"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="15dp"
            android:padding="5dp"/>
        <TextView
            android:id="@+id/L_Voltaje"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Voltaje"
            android:textColor="#97000000"
            android:textSize="12dp"
            android:textStyle="bold"
            android:background="#D8D8D8"
            android:padding="5dp"/>
        <TextView
            android:id="@+id/TB_Voltaje"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="15dp"
            android:padding="5dp"/>
        <TextView
            android:id="@+id/L_VRam"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Voltaje"
            android:textColor="#97000000"
            android:textSize="12dp"
            android:textStyle="bold"
            android:background="#D8D8D8"
            android:padding="5dp"/>
        <TextView
            android:id="@+id/TB_VRam"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="15dp"
            android:padding="5dp"/>
    </LinearLayout>
</FrameLayout>